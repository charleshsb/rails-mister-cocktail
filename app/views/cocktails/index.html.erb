<div class="container">
  <h1>List of cocktails</h1>
  <br>
  <%= link_to "Create a new cocktail", new_cocktail_path  %>
  <ul>
    <% @cocktails.each do |cocktail| %>
    <br>
      <li><%= cl_image_tag cocktail.photo.key, height: 200, width: 300, crop: :fill %><%= link_to cocktail.name, cocktail_path(cocktail) %></li>
    <% end %>
  </ul>
</div>

